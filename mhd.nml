&RUN_PARAMS
cosmo=.true.       !# Indicates RAMSES whether the simulation is cosmological (i.e. with expansion)
pic=.true.         !# Activates the particle-in-cell calculation
poisson=.true.     !# Activates the Poisson gravity solver
hydro=.true.       !# Activates the hydrodynamical solver
verbose=.false.    !# Activates the verbose output from RAMSES (slows down the code!)
ordering='hilbert' !# Hilbert curve ordering of grid cells
nrestart=999       !# Output ID to restart from
nremap=10          !# Grid remapping frequency
nsubcycle=1,1,1,2  !# Number of subcycles per level. First 3 levels are synchronised, below, 2 cycles
rt=.false.         !# No radiative transfer
sink=.true.        !# Activate sink usage
announce_dt_cond=.false. !# Activates logging of each dt limit condition
/


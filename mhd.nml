&INIT_PARAMS
aexp_ini=2d-3       !# Initial scale factor
filetype='grafic'   !# Type of initialisation files (and their path in the machine storage below)
initfile(1)='/rds/project/dirac_vol2/rds-dirac-dp012/sm2464/HabRuns/NUT_IC/IC_128'
